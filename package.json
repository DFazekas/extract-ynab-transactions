{
  "main": "index.js",
  "scripts": {
    "start": "yaml2json .env.yaml > .env.json --pretty && env-cmd -r ./.env.json functions-framework --target=extract_ynab --signature-type=http",
    "deploy": "gcloud functions deploy extract-ynab --entry-point extract_ynab --trigger-http --env-vars-file .env.yaml"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^2.1.0",
    "env-cmd": "^10.1.0",
    "yamlenv": "^1.0.0",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "axios": "^0.24.0"
  }
}